{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "strict": true,
        "allowJs": false,
        "isolatedModules": true,
        "skipLibCheck": true,
        "noEmit": true,
        "types": ["vite/client"]
    },
    "include": ["src", "vite.config.ts"]
}
